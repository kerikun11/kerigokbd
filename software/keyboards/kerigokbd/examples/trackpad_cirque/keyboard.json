{
    "keyboard_name": "KERIgoKBD Trackpad Test",
    "manufacturer": "kerikun11",
    "maintainer": "kerikun11",
    "url": "https://github.com/kerikun11/kerigokbd",
    "processor": "RP2040",
    "bootloader": "rp2040",
    "usb": {
        "vid": "0x1209",
        "pid": "0xE502",
        "device_version": "1.0.0"
    },
    "features": {
        "command": true,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "rgb_matrix": true,
        "pointing_device": true
    },
    "matrix_pins": {
        "direct": [ [ "GP1", "GP2", "GP3", "GP4", "GP5" ] ]
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [ 0, 0 ], "x": 0, "y": 0 },
                { "matrix": [ 0, 1 ], "x": 1, "y": 0 },
                { "matrix": [ 0, 2 ], "x": 2, "y": 0 },
                { "matrix": [ 0, 3 ], "x": 3, "y": 0 },
                { "matrix": [ 0, 4 ], "x": 4, "y": 0 }
            ]
        }
    },
    "ws2812": {
        "driver": "vendor",
        "pin": "GP16"
    },
    "rgb_matrix": {
        "driver": "ws2812",
        "max_brightness": 128,
        "timeout": 300000,
        "sleep": true,
        "animations": {
            "cycle_left_right": true
        },
        "layout": [
            { "matrix": [ 0, 0 ], "x":   0, "y":  0, "flags": 4 }
        ]
    }
}
